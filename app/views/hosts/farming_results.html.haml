.page-title farming results
.host-box
  .host-element
    .host-element-name
      hostname
    .host-element-value
      =@mf.hostname
  .host-element
    .host-element-name
      routes
      %br
    .host-element-value
      - @mf.routes.each do |r|
        = r
        %br
  .host-element
    .host-element-name
      firewall policy
      %br
    .host-element-value
      - @mf.firewall_policy.each do |key,val|
        = "#{key}: "
        = val
        %br

